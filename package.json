{
  "name": "@chrisfieldsii/react-components",
  "version": "0.1.0",
  "module": "dist/react-components.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "author": "Christopher Fields II",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "react-infinite-scroll-hook": "^4.0.4"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1 || ^2",
    "@tanstack/react-query": ">=4",
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@chakra-ui/react": "2.4.1",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addons": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@tanstack/react-query": "^4.17.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^9.1.0",
    "husky": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "size-limit": [
    {
      "path": "dist/react-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
